pipeline {
  agent { label 'WinPROD-agent' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'SonarDEV') { 
          echo " I am testing Sonarqube"
          sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=IFL-ITTools_JavaTest_AY468g6Qs_pWLTqP_82D"
        }
      }
    }
  }
}
